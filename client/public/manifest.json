{
  "version": "1.0.0",
  "manifest_version": 3,
  "name": "RepoRead",
  "description": "This is a chrome extension that generates ReadMe's and Repository explanations by utilizing Gemini 1.5 Pro.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "React Chrome Extension"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "downloads",
    "activeTab",
    "tabs",
    "storage",
    "scripting"
  ]
}
