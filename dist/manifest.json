{
    "version": "1.0.0",
    "manifest_version": 3,
    "name": "Github ReadMe Generator",
    "description": "This is a Chrome extension built with React and TypeScript",
    "action": {
        "default_popup": "js/index.html",
        "default_title": "React Chrome Extension"
    },
    "permissions": [
        "activeTab",
        "storage"
    ],
    "content_scripts": [
        {
          "matches": ["https://github.com/*"],
          "js": ["contentScript.js"]
        }
      ]
}